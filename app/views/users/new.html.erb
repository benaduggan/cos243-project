<h1>Sign up!</h1>

<%= form_for @user do |f| %>
  <%= f.label :usersname, "Username" %>
  <%= f.text_field :usersname %>

  <%= f.label :email, "Email" %>
  <%= f.text_field :email %>

  <%= f.label :password, "Password" %>
  <%= f.password_field :password %>

  <%= f.label :confirmation, "Confirmation" %>
  <%= f.password_field :confirmation %>

  <%= f.submit "Create new account" %>

<%end %>