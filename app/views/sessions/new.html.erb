<h1>Log In </h1>

<%if @thereisanerror%>
<div class="alert alert-danger">
    <p>Invalid</p>
</div>
<%end%>

<%= form_tag sessions_path do%>



  <%= label_tag :username %>
  <%= text_field_tag :username, params[:username], class: 'form-control' %>
  <p>
  <%= label_tag :password %>
  <%= password_field_tag :password, params[:password], class: 'form-control' %>
  </p>
    
  <p><%= submit_tag 'Log In', class: 'btn btn-primary' %></p>

<%end%>